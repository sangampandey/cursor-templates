{
  "version": 2,
  "name": "cursor-templates",
  "public": true,
  "github": {
    "silent": true
  },
  "buildCommand": "",
  "outputDirectory": "website",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "/documentation.html"
    },
    {
      "source": "/documentation",
      "destination": "/documentation.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}